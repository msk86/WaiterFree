#!/usr/bin/expect

set timeout 1000
set email "$env(METEOR_STAGING_EMAIL)"
set password "$env(METEOR_STAGING_PASSWORD)"

spawn METEOR_SETTINGS="{\"public\":{\"a\":1}}" meteor deploy $env(METEOR_DEPLOY_URL)

#puts $command
expect -- "*Email:"
send "$email\n"
expect -- "*assword:"
send -- "$password\n"
expect -- "*Now serving at $env(METEOR_DEPLOY_URL)"
